  "NameLine": "(?im)^(?:Patient|Subscriber|Member|Provider)\\s*(?:Name)?[:\\s]+([A-Z][A-Za-z'\\-]+(?:\\s+[A-Z][A-Za-z'\\-]+){1,3})$"
