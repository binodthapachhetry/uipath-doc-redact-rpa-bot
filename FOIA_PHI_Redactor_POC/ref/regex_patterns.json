  "DeviceSerial": "\\b(?:Device|Serial|S\\/N|SN)[:\\s]*([A-Z0-9\\-]{6,})\\b",

  "URL": "\\bhttps?:\\/\\/[\\w\\-\\.]+(?:\\/[\\w\\-\\.\\/%?=&#]*)?\\b",
